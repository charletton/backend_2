<h1>Register</h1>

<form id="registerForm">
    <label>Name</label>
    <input name="firstName">
    <br/>
    <label>Last name</label>
    <input name="lastName">
    <br/>
    <label>Email</label>
    <input name="email">
    <br/>
    <label>Birth date</label>
    <input type="date" name="birthDate">
    <br/>
    <label>Password</label>
    <input name="password">
    <br/>
    <input type="submit">
</form>

<script src="/js/register.js"></script>

